#include<iostream>
using namespace std;

int main()
{
    
    // 321
    // 321
    // 321
   
   int x;
   cout<<"Enter the number: ";
   cin>>x;
   
    int rows=1;
    while(rows<=x){
        int columns=3;
        while(columns>=1){
            cout<<columns;
            columns--;
        }
        cout<<endl;
        rows++;
    }
    
}
