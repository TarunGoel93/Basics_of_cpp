#include<iostream>
using namespace std;

int main()
{

int ctr=0,i,num;

cout<<"Enter the number : ";
cin>>num;

for(i=1;i<=num;i++){
    if(num%i==0){
        ctr++;
    }
}

if(ctr==2){
    cout<<"Entered number is a prime number"<<endl;
}
else{
    cout<<"Entered number is not a prime number"<<endl;
}
    
return 0;
}
