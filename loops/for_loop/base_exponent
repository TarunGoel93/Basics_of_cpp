#include<iostream>
using namespace std;

int main()
{
    
    int base,exponent,result=1;
    
    cout<<"Value od Base : ";
    cin>>base;
    cout<<"Value od Exponent : ";
    cin>>exponent;
    
    for(int i=0;i<exponent;i++){
        result*=base;
    }
    
    cout<<result;
    
return 0;    
}
