#include<iostream>
using namespace std;

int main()
{
    int x;
    
    cout<<"Enter a number to genrate its multilplication table : ";
    cin>>x;
    cout<<"Multiplication table for "<<x<<" : "<<endl;
    
    for(int i=1;i<=10;i++){
        cout<<x<<" x "<<i<<" = "<<x*i<<endl;
    }
    
return 0;
}
