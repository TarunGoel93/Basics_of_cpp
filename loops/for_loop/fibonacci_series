#include<iostream>
using namespace std;

int main()
{
    int x=0,y=1,n;
    
    cout<<"Enter the number of terms for the Fibonacci series : ";
    cin>>n;
    
    int next;
    
    for(int i=1;i<=n;i++){
        next=x+y;
        x=y;
        y=next;
        cout<<"Fibonacci Series up to "<<n<<" terms : "<<next<<endl;
    }
    
return 0;
}
