#include<iostream>
using namespace std;

int main()
{
    int size;

    cout<<"Enter the size of the array : ";
    cin>>size;
    
    int arr[size];
     
    cout<<"Enter the elements : "<<endl;
    
    for(int i=0;i<size;i++){
       cin>>arr[i];
    }

    cout<<"The array elemnt is : "<<endl;
    
    for(int i=0;i<size;i++){
        cout<<arr[i]<<" ";
    }
    
    cout<<endl;
    
return 0;    
}
